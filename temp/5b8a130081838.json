select name=\"gh_injury_comp_3\" onchange=\"toggleSection(this.value === 'Yes', 'gh_injury_employer_3')\">\n                    <option value=\"\"><\/option>\n                    <option value=\"Yes\">Yes<\/option>\n                    <option value=\"No\">No<\/option>\n                  <\/select>\n                <\/td>\n                <td>\n                  <div data-toggle-section=\"gh_injury_employer_3\">\n                    <input type=\"text\" name=\"gh_injury_employer_3\" maxlength=\"35\">\n                  <\/div>\n                <\/td>\n                <td>\n                  <button class=\"btn btn-sm btn-danger btn-remove-injury\" onclick=\"return removeInjury()\">Remove<\/button>\n                <\/td>\n              <\/tr>\n              <tr data-injury=\"4\">\n                <td>\n                  <input type=\"text\" name=\"gh_injury_name_4\" maxlength=\"28\">\n                <\/td>\n                <td>\n                  <input type=\"text\" name=\"gh_injury_date_4\" data-rule-date=\"true\"  data-msg-date=\"Please enter date in the format MM\/DD\/YYYY\" placeholder=\"MM\/DD\/YYYY\"  data-mask=\"00\/00\/0000\" >\n                <\/td>\n                <td>\n                  <input type=\"text\" name=\"gh_injury_physician_4\" maxlength=\"35\">\n                <\/td>\n                <td>\n                  <select name=\"gh_injury_comp_4\" onchange=\"toggleSection(this.value === 'Yes', 'gh_injury_employer_4')\">\n                    <option value=\"\"><\/option>\n                    <option value=\"Yes\">Yes<\/option>\n                    <option value=\"No\">No<\/option>\n                  <\/select>\n                <\/td>\n                <td>\n                  <div data-toggle-section=\"gh_injury_employer_4\">\n                    <input type=\"text\" name=\"gh_injury_employer_4\" maxlength=\"35\">\n                  <\/div>\n                <\/td>\n                <td>\n                  <button class=\"btn btn-sm btn-danger btn-rem